<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Plex">

    <Color x:Key="BackgroundDark">#373838</Color>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}" />

    <Color x:Key="BackgroundLighterDark">#4a4a4a</Color>
    <SolidColorBrush x:Key="BackgroundLighterDarkBrush" Color="{StaticResource BackgroundLighterDark}" />

    <Color x:Key="BackgroundLightestDark">#5a5b5b</Color>
    <SolidColorBrush x:Key="BackgroundLightestDarkBrush" Color="{StaticResource BackgroundLightestDark}" />

    <Color x:Key="ForegroundColor">WhiteSmoke</Color>
    <SolidColorBrush x:Key="ForegroundColorBrush" Color="{StaticResource ForegroundColor}" />
    
    

</ResourceDictionary>